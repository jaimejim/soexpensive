{
  "rewrites": [
    {
      "source": "/scraper",
      "destination": "/scraper.html"
    },
    {
      "source": "/admin",
      "destination": "/admin.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600, must-revalidate"
        }
      ]
    }
  ],
  "functions": {
    "api/fetch-prices.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "git": {
    "deploymentEnabled": {
      "claude/price-comparison-app-km6Wj": true
    }
  }
}
